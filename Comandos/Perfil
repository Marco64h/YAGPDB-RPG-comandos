//Este comando permite que vocÃª veja todos os seus stats salvos, nome, foto e a cor definida pelos outros comandos.
//Caso use o mesmo prefixo e queira ver seu prÃ³prio profile: <profile
//Caso queira ver o profile de outra pessoa: <profile @UsuÃ¡rio
//Caso queira ver o seu profile ou de outra pessoa que esta salvo em outro SLOT, basta usar: <profile @UsuÃ¡rio 2

{{$ln := (len .Args)}}
{{$nil := (joinStr "" "[REDACTED]")}}
	{{if gt $ln 2}}
	{{$2 := (index .Args 2)}}
	{{$profileK := printf "profile%d" (toInt $2)}}
	{{$1 := (index .Args 1)}}
		{{if ($user := userArg $1)}}
		{{$id := toInt64 $user.ID}}
			{{if (dbGet $id $profileK)}}
			{{$profileD := (dbGet $id $profileK).Value}}
			{{$stats := sdict $profileD}}

			{{$color := randInt 0 16777215}}
			{{$colorcheck := ($stats.Get "cor")}}
				{{if $colorcheck}}
				{{$color = ($stats.Get "cor")}}
				{{end}}
			{{$color = toInt $color}}

				{{$embed := cembed "title" (joinStr "" ($stats.Get "nome")) "description" (joinStr "" "**```ansi\n[2;30mâ”â”â”â”…â”…â”…â”„â”„âŸâŸ¦[2;33mâœ®[2;30mâŸ§âŸâ”„â”„â”‰â”‰â”…â”â”â”“\n[2;30mâ”£ğŸ’—[2;38mâŠ°â©¥[2;30mâª[2;35mHP[2;30mâ«ã€[2;31m" ($stats.Get "HP") " [2;30m| [2;31m" ($stats.Get "MaxHP") "[2;30mã€\nâ”£ğŸ”µ[2;38mâŠ°â©¥[2;30mâª[2;36mMP[2;30mâ«ã€[2;34m" ($stats.Get "Energia") " [2;30m| [2;34m" ($stats.Get "MaxEnergia") "[2;30mã€\nâ”£ğŸ’ [2;38mâŠ°â©¥[2;30mâª[2;32mRD[2;30mâ«ã€[2;33m" ($stats.Get "RD") "% [2;30m| [2;33m" ($stats.Get "Escudo") "[2;30mã€\nâ”—â”â”â”…â”…â”…â”„â”„âŸâŸ¦[2;33mâœ®[2;30mâŸ§âŸâ”„â”„â”‰â”‰â”…â”â”â”›```**" "\n**```ansi\n[2;36mğŸŒâŠ°â©¥[2;30mã€[2;33mğğ¢ğ¯ğğ¥: [2;37m" ($stats.Get "Nivel") "[2;30mã€‘\nğŸ§ [2;36mâŠ°â©¥[2;30mã€[2;35mğ’ğšğ§ğ¢ğğšğğ: [2;37m" ($stats.Get "Sanidade") "[2;30mã€‘```**" "\n**```ansi\n[2;30mâ•­â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â•®\n[2;30mâ”‚ğŸ‘Š[2;38mâŠ°â©¥[2;34mã€Œ[2;31mForÃ§a: [2;37m" ($stats.Get "DanoFis") "\n[2;30mâ”‚âœ¨ï¸[2;38mâŠ°â©¥[2;34mã€Œ[2;36mPoder: [2;37m" ($stats.Get "DanoMa") "\n[2;30mâ•°â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â•¯```**") "thumbnail" (sdict "url" ($stats.Get "foto")) "color" $color}}
				{{sendMessage nil $embed}}
			{{else}}
			{{$msg1 := (joinStr "" "**NÃ£o hÃ¡ nenhum um profile salvo neste Slot! __" $profileK "__**")}}
			{{sendMessage nil $msg1}}
			{{end}}
		{{else}}> **MenÃ§Ã£o invÃ¡lida!**
		{{end}}

	{{else}}
		{{if gt $ln 1}}
		{{$1 := (index .Args 1)}}
		{{$user := userArg $1}}
		{{$id := toInt64 $user.ID}}
